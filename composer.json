    "keywords": ["id3", "id3tag"],
    "homepage": "https://github.com/rhorber/id3rw",
    "readme": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Raphael Horber",
            "email": "raphael.horber@gmail.com",
